    "license": "ISC",
    "dependencies": {},
    "devDependencies": {
        "babel-cli": "^6.23.0",
        "babel-preset-env": "^1.1.8",
        "eslint": "^3.15.0",
        "eslint-plugin-jest": "^1.0.2",
        "jest": "^18.1.0",
        "uglify-js": "^2.7.5"
    },
    "babel": {
        "presets": ["env"]
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/zgulde/my-js-lib"
    }
}
